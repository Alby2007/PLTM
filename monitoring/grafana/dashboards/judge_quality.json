{
  "dashboard": {
    "title": "Judge Quality Monitoring",
    "tags": ["metajudge", "observability", "judges"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Judge Accuracy Over Time",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "judge_accuracy",
            "legendFormat": "{{judge_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Accuracy",
            "min": 0,
            "max": 1
          }
        ],
        "tooltip": {
          "shared": true,
          "sort": 2,
          "value_type": "individual"
        },
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "min": true,
          "values": true
        }
      },
      {
        "id": 2,
        "title": "Judge Evaluations by Verdict",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(judge_evaluations_total[5m])",
            "legendFormat": "{{judge_name}} - {{verdict}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Evaluations/sec"
          }
        ],
        "tooltip": {
          "shared": true,
          "sort": 2,
          "value_type": "individual"
        }
      },
      {
        "id": 3,
        "title": "Judge Latency (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(judge_latency_seconds_bucket[5m]))",
            "legendFormat": "{{judge_name}} - p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(judge_latency_seconds_bucket[5m]))",
            "legendFormat": "{{judge_name}} - p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(judge_latency_seconds_bucket[5m]))",
            "legendFormat": "{{judge_name}} - p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency",
            "logBase": 1
          }
        ],
        "tooltip": {
          "shared": true,
          "sort": 2,
          "value_type": "individual"
        },
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "values": true
        }
      },
      {
        "id": 4,
        "title": "Conflict Detection Rate by Judge",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "judge_conflict_detection_rate",
            "legendFormat": "{{judge_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Detection Rate",
            "min": 0,
            "max": 1
          }
        ],
        "tooltip": {
          "shared": true,
          "sort": 2,
          "value_type": "individual"
        },
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "values": true
        }
      },
      {
        "id": 5,
        "title": "Judge Confidence Distribution",
        "type": "heatmap",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(judge_confidence_bucket[5m])",
            "legendFormat": "{{judge_name}}",
            "refId": "A"
          }
        ],
        "dataFormat": "tsbuckets",
        "yAxis": {
          "format": "short",
          "label": "Confidence Score"
        },
        "color": {
          "mode": "spectrum",
          "cardColor": "#b4ff00",
          "colorScale": "sqrt",
          "exponent": 0.5,
          "colorScheme": "interpolateSpectral"
        }
      },
      {
        "id": 6,
        "title": "Top Performing Judges (by Accuracy)",
        "type": "table",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "topk(5, judge_accuracy)",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "judge_name": 0,
                "Value": 1
              },
              "renameByName": {
                "judge_name": "Judge",
                "Value": "Accuracy"
              }
            }
          }
        ],
        "styles": [
          {
            "pattern": "Accuracy",
            "type": "number",
            "unit": "percentunit",
            "decimals": 2
          }
        ]
      },
      {
        "id": 7,
        "title": "Judge Comparison Matrix",
        "type": "table",
        "gridPos": {"x": 0, "y": 24, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "judge_accuracy",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "judge_conflict_detection_rate",
            "format": "table",
            "instant": true,
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(judge_latency_seconds_bucket[5m]))",
            "format": "table",
            "instant": true,
            "refId": "C"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "judge_name": 0,
                "Value #A": 1,
                "Value #B": 2,
                "Value #C": 3
              },
              "renameByName": {
                "judge_name": "Judge",
                "Value #A": "Accuracy",
                "Value #B": "Detection Rate",
                "Value #C": "p95 Latency"
              }
            }
          }
        ],
        "styles": [
          {
            "pattern": "Accuracy",
            "type": "number",
            "unit": "percentunit",
            "decimals": 2,
            "colorMode": "cell",
            "colors": ["rgba(245, 54, 54, 0.9)", "rgba(237, 129, 40, 0.89)", "rgba(50, 172, 45, 0.97)"],
            "thresholds": ["0.7", "0.9"]
          },
          {
            "pattern": "Detection Rate",
            "type": "number",
            "unit": "percentunit",
            "decimals": 2,
            "colorMode": "cell",
            "colors": ["rgba(245, 54, 54, 0.9)", "rgba(237, 129, 40, 0.89)", "rgba(50, 172, 45, 0.97)"],
            "thresholds": ["0.3", "0.6"]
          },
          {
            "pattern": "p95 Latency",
            "type": "number",
            "unit": "s",
            "decimals": 3,
            "colorMode": "cell",
            "colors": ["rgba(50, 172, 45, 0.97)", "rgba(237, 129, 40, 0.89)", "rgba(245, 54, 54, 0.9)"],
            "thresholds": ["0.05", "0.1"]
          }
        ]
      },
      {
        "id": 8,
        "title": "Total Evaluations by Judge",
        "type": "stat",
        "gridPos": {"x": 0, "y": 32, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(judge_evaluations_total) by (judge_name)",
            "legendFormat": "{{judge_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "auto",
          "textMode": "auto"
        }
      },
      {
        "id": 9,
        "title": "Average Judge Accuracy",
        "type": "stat",
        "gridPos": {"x": 6, "y": 32, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "avg(judge_accuracy)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "auto",
          "textMode": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Best Conflict Detector",
        "type": "stat",
        "gridPos": {"x": 12, "y": 32, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "topk(1, judge_conflict_detection_rate)",
            "legendFormat": "{{judge_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value",
          "orientation": "auto",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 11,
        "title": "Fastest Judge (p95 Latency)",
        "type": "stat",
        "gridPos": {"x": 18, "y": 32, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "bottomk(1, histogram_quantile(0.95, rate(judge_latency_seconds_bucket[5m])))",
            "legendFormat": "{{judge_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value",
          "orientation": "auto",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3
          }
        }
      }
    ],
    "refresh": "10s",
    "schemaVersion": 27,
    "style": "dark",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}
